<!DOCTYPE html>
<html lang="pt-BR">
 <%- include('partials/head.ejs') %>
<body>
    
    <h1><%= title %></h1>
    
    <%- include('partials/navbar.ejs') %>

    <ul>
        <% users.forEach((user) => { %>
            <li>
                Nome: <%= user.name %> - Email: <%= user.email %> - Idade: <%= user.age %> -
                <a href="/edit?id=<%=user._id%>">[Editar]</a> -
                <a href="/remove/<%=user._id%>">[remover]</a>
            </li>
        <% }); %>
    </ul>

    <%- include('partials/foot.ejs') %>
</body>
</html>